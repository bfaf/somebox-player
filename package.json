{
  "name": "someboxplayer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "react-native start",
    "start:rc": "react-native start --reset-cache",
    "build": "cd android && gradlew build",
    "test": "jest",
    "format": "prettier **/*.{js,ts,tsx} --check --write",
    "format:check": "prettier **/*.{js,ts,tsx} --check",
    "typecheck": "tsc --noEmit",
    "clean": "cd android && gradlew clean",
    "assemble:debug": "cd android && gradlew assembleDebug",
    "assemble:release": "cd android && gradlew clean assembleRelease",
    "deploy:android:tablet": "adb -s HA1A7HWY install ./android/app/build/outputs/apk/release/app-universal-release.apk",
    "web": "webpack-dev-server --config ./web/webpack.config.js --open",
    "build:web": "webpack --config ./web/webpack.config.prod.js",
    "build:docker": "docker build -t somebox-web-player:latest ."
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-native-fontawesome": "^0.3.2",
    "@mui/material": "^5.14.11",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-material/core": "^1.3.7",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "npm:react-native-tvos@0.75.2-0",
    "react-native-media-console": "^2.2.4",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "15.4.0",
    "react-native-video": "^6.7.0",
    "react-native-web": "^0.19.13",
    "react-player": "^2.16.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "react-router-native": "^6.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^15.0.0",
    "@react-native-community/cli-platform-android": "^15.0.0",
    "@react-native-community/cli-platform-ios": "^15.0.0",
    "@react-native/babel-preset": "^0.76.1",
    "@react-native/eslint-config": "^0.76.1",
    "@react-native/metro-config": "^0.76.1",
    "@react-native/typescript-config": "^0.76.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-native-web": "^0.19.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-native-web": "^0.19.13",
    "eslint": "^8.19.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react-native-gesture-handler": "^2.20.2",
    "react-test-renderer": "^18.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "node": ">=18"
  }
}
